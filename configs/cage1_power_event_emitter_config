export _type=power
export _group=ff15::11
export _interface=enp4s3
export _port=5001
export _caps='application/x-rtp, media=audio, clock-rate=48000, encoding-name=L16, encoding-params=1, payload=96, format=S16BE, rate=48000, channels=1, layout=interleaved'
export GST_VAAPI_ALL_DRIVERS=1
export GST_GL_WINDOW=gbm
export _entrypoint="gst-launch-1.0 -q udpsrc multicast-group=$_group multicast-iface=$_interface port=$_port auto-multicast=true ! $_caps ! rtpL16depay ! audioconvert ! powereventemitter ! fdsink | stdbuf -o0 tr -d '\000' | /Services/power_event_emitter/bin/event_parser.py $_cage $_type | kafkacat -b Kafka01,Kafka02 -t power_event_emitter"


