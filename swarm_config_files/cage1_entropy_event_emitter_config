export _cage=1
export _type=entropy
export _upwards_threshold=30
export _downwards_threshold=20
export _event_on_high=False
export _group=ff15::11
export _interface=enp4s3
export _port=5004
export _caps='application/x-rtp, media=audio, clock-rate=48000, encoding-name=L16, encoding-params=1, payload=96, format=S16BE, rate=48000, channels=2, layout=interleaved'
export GST_VAAPI_ALL_DRIVERS=1
export GST_GL_WINDOW=gbm
export _entrypoint="gst-launch-1.0 -q udpsrc multicast-group=$_group multicast-iface=$_interface port=$_port auto-multicast=true ! $_caps ! rtpL16depay ! 'audio/x-raw, format=S16BE, layout=interleaved, rate=48000, channels=1' ! audioconvert ! entropyeventemitter ! fdsink | stdbuf -o0 tr -d '\000' | /scripts/event_emitter.py $_cage $_type $_upwards_threshold $_downwards_threshold $_event_on_high"


